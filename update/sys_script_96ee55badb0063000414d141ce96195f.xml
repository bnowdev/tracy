        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
		
	wait(5000);
	
	var trz = new x_trz.TracyService();
	trz.updateVersion(current);
	
	
	
	
 /*A custom gs.sleep() as it is not implemented in scoped applications.
   time is in milliseconds - this is essentially a "sleep" function */   
   function wait(time){   
	   var t1 = new GlideDateTime().getNumericValue();   
	   var t2 = new GlideDateTime().getNumericValue();   
	   var duration = t2 - t1;   
	   while(duration < time){   
		   t2 = new GlideDateTime().getNumericValue();   
		   duration = t2 - t1;   
	   }   
	}

})(current, previous);]]></script>
